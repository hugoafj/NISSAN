<Alloy>
	<Window class="container">
		<View class="boxcontainer">
			<View class="options">
				<View class="option" onClick="openServicios">
					<Label class="labelOption">Citas de Servicios</Label>
					<View class="vertLine"></View>
					<View class="row"></View>
				</View>
				<View class="option" onClick="openEstadoCitas">
					<Label class="labelOption">Estado de mis Citas</Label>
					<View class="vertLine"></View>
					<View class="row"></View>
				</View>
				<View class="option" onClick="openNotificaciones">
					<Label class="labelOption">Eventos</Label>
					<View class="vertLine"></View>
					<View class="row"></View>
				</View>
				<View class="option" onClick="openPruebaManejo">
					<Label class="labelOption">Pruebas de manejo</Label>
					<View class="vertLine"></View>
					<View class="row"></View>
				</View>
				<View class="option" onClick="openGaleria">
					<Label class="labelOption">Galeria de autos</Label>
					<View class="vertLine"></View>
					<View class="row"></View>
				</View>
				<View class="option" onClick="openContacto">
					<Label class="labelOption" >Contacto</Label>
					<View class="vertLine"></View>
					<View class="row"></View>
				</View>
				<View class="option" onClick="openLogin">
					<Label class="labelOption" >Cerrar Sesion</Label>
				</View>
			</View>
		</View>
		<View class="menuBar">
			<View class="facebook" onClick="openFace">
				<ImageView class="iconFacebook"></ImageView>
				<Label class="label">facebook</Label>
			</View>
			<View class="lonuevo" onClick="loNuevo">
				<ImageView class="iconLoNuevo"></ImageView>
				<Label class="label">lo nuevo</Label>
			</View>
			<View class="eventos">
				<ImageView class="iconEventos" onClick="openNotificaciones"></ImageView>
				<Label class="label">eventos</Label>
			</View>
			<View class="mas" onClick="openMas">
				<ImageView class="iconMas"></ImageView>
				<Label class="label">mas</Label>
			</View>
		</View>
	</Window>
</Alloy>